generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // hoặc "mysql", "sqlite" tùy DB bạn dùng
  url      = env("DATABASE_URL")
}

model User {
  id                  String  @id @default(uuid())
  address             String  @unique
  encryptedPrivateKey String?
}
